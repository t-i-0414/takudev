  "scripts": {
    "build": "turbo run build",
    "build:graph": "turbo run build --graph",
    "dev": "turbo run dev",
    "format": "run-p \"format:*\"",
    "format:eslint": "eslint --fix './*.{js,ts}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "prettier --cache --write './*' --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\"",
    "format:turbo": "turbo run format",
    "lint": "run-p \"lint:*\"",
    "lint:eslint": "eslint './*.{js,ts}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "prettier --cache --check './*' --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "sort-package-json --check \"package.json\"",
    "lint:turbo": "turbo run lint",
    "prepare": "husky install",
    "type-check": "turbo run type-check",
    "typesync": "pnpm dlx typesync",
    "upgrade-interactive": "pnpm up --recursive --latest --interactive"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@takudev/eslint-config-custom": "workspace:*",
    "@turbo/gen": "^1.10.12",
    "@types/eslint": "^8.44.1",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/lint-staged": "^13.2.0",
    "@types/node": "^17.0.12",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-turbo": "^1.10.12",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "npm-check": "^6.0.1",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "sort-package-json": "^2.5.1",
    "turbo": "1.10.12",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@8.6.8"
}
