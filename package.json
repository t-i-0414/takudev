{
  "name": "takudev",
  "version": "1.0.0",
  "private": true,
  "description": "This is a package of t-i-0414's blog.",
  "homepage": "https://github.com/t-i-0414/takudev#readme",
  "bugs": {
    "url": "https://github.com/t-i-0414/takudev/issues",
    "email": "gutlo.ol0414@gmail.com"
  },
  "repository": "git@github.com:t-i-0414/takudev.git",
  "license": "UNLICENSED",
  "author": "t-i-0414 <gutlo.ol0414@gmail.com> (https://github.com/t-i-0414/)",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:graph": "turbo run build --graph",
    "format": "run-p \"format:*\"",
    "format:eslint": "eslint --fix './*.{js,ts}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "prettier --cache --write './*' --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\"",
    "format:turbo": "turbo run format",
    "lint": "run-p \"lint:*\"",
    "lint:commit": "commitlint -f HEAD",
    "lint:eslint": "eslint './*.{js,ts}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "prettier --cache --check './*' --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "sort-package-json --check \"package.json\"",
    "lint:turbo": "turbo run lint",
    "prepare": "husky install",
    "test": "turbo run test",
    "test:vscode-jest": "jest",
    "type-check": "turbo run type-check",
    "typesync": "npx typesync"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.4.1",
    "turbo": "^1.7.0",
    "typescript": "4.9.4",
    "typesync": "^0.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.2.5",
    "@types/node": "18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "find-yarn-workspace-root": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "react-test-renderer": "^18.2.0",
    "sort-package-json": "^2.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
