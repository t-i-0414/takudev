{
  "name": "takudev",
  "version": "1.0.0",
  "private": true,
  "description": "This is a package of t-i-0414's blog.",
  "homepage": "https://github.com/t-i-0414/takudev#readme",
  "bugs": {
    "url": "https://github.com/t-i-0414/takudev/issues",
    "email": "gutlo.ol0414@gmail.com"
  },
  "repository": "git@github.com:t-i-0414/takudev.git",
  "license": "UNLICENSED",
  "author": "t-i-0414 <gutlo.ol0414@gmail.com> (https://github.com/t-i-0414/)",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "commitlint": "commitlint -f HEAD",
    "format": "turbo run format",
    "postinstall": "typesync",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "sort-package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "test": "turbo run test",
    "type-check": "turbo run type-check"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/react": "^17.0.37",
    "dotenv": "^10.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "typesync": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/eslint": "^8.2.1",
    "@types/jest": "^27.0.3",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.3",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.53.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "turbo": "^1.0.28"
  },
  "turbo": {
    "baseBranch": "origin/develop",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      },
      "test": {
        "dependsOn": [
          "build"
        ]
      },
      "format": {},
      "lint": {},
      "type-check": {}
    }
  },
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  }
}
