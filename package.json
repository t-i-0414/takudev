    "codegen:graphql": "graphql-codegen --config src/graphql_schema/codegen.yaml",
    "dev": "next dev",
    "format": "run-p \"format:*\"",
    "format:eslint": "eslint --fix './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "prettier . --cache --write --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\"",
    "format:stylelint": "stylelint --fix '**/*.{css,scss}' --cache --ignore-path ./.stylelintignore",
    "lint": "run-p \"lint:*\"",
    "lint:commit": "commitlint -f HEAD",
    "lint:eslint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "prettier . --cache --check --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "sort-package-json --check \"package.json\"",
    "lint:stylelint": "stylelint '**/*.{css,scss}' --cache --ignore-path ./.stylelintignore",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci --no-open",
    "storycap": "storycap --serverCmd 'run-p storybook:ci' --serverTimeout 300000 --outDir ./__screenshots__/ http://localhost:6006",
    "test": "jest",
    "test:storybook": "test-storybook",
    "test:vscode-jest": "jest",
    "type-check": "yarn tsc --noEmit",
    "typesync": "npx typesync"
  },
