{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "noEmit": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    },
    "allowJs": false
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".**/*.ts", ".**/*.tsx"],
  "exclude": ["node_modules"]
}
