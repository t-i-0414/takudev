    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@next/bundle-analyzer": "^13.4.2",
    "@storybook/addon-actions": "^7.0.11",
    "@storybook/addon-essentials": "^7.0.11",
    "@storybook/addon-interactions": "^7.0.11",
    "@storybook/addon-links": "^7.0.11",
