  "scripts": {
    "build": "next build",
    "build:storybook": "build-storybook --output-dir ./dist-storybook",
    "dev": "next dev",
    "format": "yarn run -T run-p \"format:*\"",
    "format:eslint": "yarn run -T eslint --fix './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "yarn run -T prettier . --cache --write --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "yarn run -T sort-package-json \"package.json\"",
    "format:stylelint": "stylelint --fix '**/*.{css,scss}' --cache --ignore-path ./.stylelintignore",
    "lint": "yarn run -T run-p \"lint:*\"",
    "lint:eslint": "yarn run -T eslint './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "yarn run -T prettier . --cache --check --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "yarn run -T sort-package-json --check \"package.json\"",
    "lint:stylelint": "stylelint '**/*.{css,scss}' --cache --ignore-path ./.stylelintignore",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "storycap": "storycap http://localhost:6006 --outDir ./__screenshots__/ --serverCmd 'start-storybook -p 6006' --serverTimeout 60000",
    "test": "yarn run -T jest",
    "test:storybook": "test-storybook",
    "type-check": "yarn run -T tsc --noEmit"
  },
  "browserslist": [
    "defaults"
  ],
