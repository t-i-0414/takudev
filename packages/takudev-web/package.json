{
  "name": "@takudev/web",
  "version": "1.5.0",
  "private": true,
  "description": "This is a package of t-i-0414's blog related web.",
  "homepage": "https://github.com/t-i-0414/takudev/tree/main/packages/takudev-web#readme",
  "license": "UNLICENSED",
  "author": "t-i-0414 <gutlo.ol0414@gmail.com> (https://github.com/t-i-0414/)",
  "scripts": {
    "build": "next build && next-sitemap",
    "build:analyze": "ANALYZE=true yarn build",
    "build:storybook": "storybook build --output-dir ./dist-storybook",
    "codegen:graphql": "graphql-codegen --config graphql_schema/codegen.yaml",
    "dev": "next dev",
    "format": "yarn run -T run-p \"format:*\"",
    "format:eslint": "yarn run -T eslint --fix './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "yarn run -T prettier . --cache --write --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "yarn run -T sort-package-json \"package.json\"",
    "format:stylelint": "stylelint --fix '**/*.{css,scss}' --cache --ignore-path ./.stylelintignore",
    "lint": "yarn run -T run-p \"lint:*\"",
    "lint:eslint": "yarn run -T eslint './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "yarn run -T prettier . --cache --check --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "yarn run -T sort-package-json --check \"package.json\"",
    "lint:stylelint": "stylelint '**/*.{css,scss}' --cache --ignore-path ./.stylelintignore",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci --no-open",
    "storycap": "storycap --serverCmd 'yarn run -T run-p storybook:ci' --serverTimeout 300000 --outDir ./__screenshots__/ http://localhost:6006",
    "test": "yarn run -T jest",
    "test:storybook": "test-storybook",
    "type-check": "yarn run -T tsc --noEmit"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "body-scroll-lock": "^4.0.0-beta.0",
    "buffer": "^6.0.3",
    "feed": "^4.2.2",
    "focus-trap": "^7.4.1",
    "graphql": "^16.6.0",
    "marked": "^5.0.2",
    "next": "13.4.2",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.0.9",
    "react": "18.2.0",
