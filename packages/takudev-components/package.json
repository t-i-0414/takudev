{
  "name": "@takudev/components",
  "version": "1.0.0",
  "private": true,
  "description": "This is a package of t-i-0414's blog related components.",
  "homepage": "https://github.com/t-i-0414/takudev/tree/main/packages/takudev-components#readme",
  "license": "UNLICENSED",
  "author": "t-i-0414 <gutlo.ol0414@gmail.com> (https://github.com/t-i-0414/)",
  "main": "./src/index.ts",
  "scripts": {
    "build-storybook": "build-storybook",
    "format": "run-p format:*",
    "format:eslint": "eslint --fix './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ../../.eslintignore",
    "format:prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\"",
    "postinstall": "typesync",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ../../.eslintignore",
    "lint:prettier": "prettier . --check --ignore-path ../../.prettierignore",
    "lint:sort-package-json": "sort-package-json --check \"package.json\"",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-path ../../.eslintignore",
      "eslint --ignore-path ../../.eslintignore"
    ],
    "*": [
      "prettier --write --ignore-path ../../.prettierignore",
      "prettier --check --ignore-path ../../.prettierignore"
    ],
    "./package.json": [
      "sort-package-json",
      "sort-package-json --check"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-jsx": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/babel__core": "^7.1.18",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/throttle-debounce": "^2.1.0",
    "@types/webpack": "5.28.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-require": "^3.1.3",
    "esbuild": "^0.14.21",
    "esbuild-register": "^3.3.2",
    "eslint-plugin-testing-library": "^5.0.3",
    "throttle-debounce": "^3.0.1",
    "webpack": "4.46.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
