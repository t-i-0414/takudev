  "scripts": {
    "format": "run-p \"format:*\"",
    "format:eslint": "eslint --fix './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "prettier . --cache --write --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\"",
    "generate:component": "turbo gen react-component",
    "lint": "run-p \"lint:*\"",
    "lint:eslint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "prettier . --cache --check --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "sort-package-json --check \"package.json\"",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@takudev/eslint-config-custom": "workspace:*",
    "@takudev/tsconfig": "workspace:*",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "eslint": "^8.44.0",
    "react": "^18.2.0",
    "typescript": "^5.1.6"
  }
