{
  "name": "@takudev/eslint-config-custom",
  "version": "2.0.0",
  "private": true,
  "description": "This is a custom eslint config package of takudev project.",
  "homepage": "https://github.com/t-i-0414/takudev/tree/main/packages/eslint-config-custom",
  "license": "UNLICENSED",
  "author": "t-i-0414 <gutlo.ol0414@gmail.com> (https://github.com/t-i-0414/)",
  "main": "index.js",
  "scripts": {
    "format": "run-p \"format:*\"",
    "format:eslint": "eslint --fix './**/*.{js,ts}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "prettier . --cache --write --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\"",
    "lint": "run-p \"lint:*\"",
    "lint:eslint": "eslint './**/*.{js,ts}' --cache --ignore-path ./.eslintignore",
    "lint:prettier": "prettier . --cache --check --ignore-unknown --ignore-path ./.prettierignore",
    "lint:sort-package-json": "sort-package-json --check \"package.json\""
  },
  "dependencies": {
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-turbo": "^1.10.7",
    "eslint-plugin-react": "7.32.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
