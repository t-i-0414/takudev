{
  "name": "eslint-config-custom",
  "version": "0.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "format": "run-p \"format:*\"",
    "format:eslint": "eslint --fix './**/*.{js,jsx,ts,tsx}' --cache --ignore-path ./.eslintignore",
    "format:prettier": "prettier . --cache --write --ignore-unknown --ignore-path ./.prettierignore",
    "format:sort-package-json": "sort-package-json \"package.json\""
  },
  "dependencies": {
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-turbo": "^1.10.7",
    "eslint-plugin-react": "7.32.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
